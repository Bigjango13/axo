use std
use testing

fn main([][]byte args){
    printf("Hello world\n")
    testing.run("test.axo", "test_addition")
    ret 0
}

fn nice([][]byte args){
    for each [i], arg in args {
        printf("arg #%d: %s\n", i, arg.data)
    }
    ret 0
}

fn testing.addition([][]byte args){
    printf("Testing addition...\n")
    ret 1+1==2 ? 0 : 1
}