#include <stdio.h>
#include "./axo/axo_include.h"
#register int printf(char*, ...)

fn main([][]char args){
    code = ":)"
    arr = [0,1,4,9,16,25,36,49,64,81,100]
    arr2 = [4|3][
        1,2,3,4,
        2,4,6,8,
        3,6,9,12
    ]

    //Perform tests
    printf("Test #1:\n")
    for each [i] in arr {
        printf("%d^2=%d\n", i, i*i)
    }

    printf("\nTest #2:\n")
    for each [x|y] in arr2 {
        printf("[%d|%d]\n", x, y)
    }

    printf("\nTest #3:\n")
    i = 0
    for each v in arr2 {
        printf("%d:%d\n", i++, v)
    }

    printf("\nTest #4:\n")
    for each [x|] in arr2 {
        for each [x|y], val in arr2 {
            printf("%d ", val)
        }
        printf("\n")
    }

    ret 0
}
